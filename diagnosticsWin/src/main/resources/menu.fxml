<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox id="vbox" prefHeight="24.0" prefWidth="772.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="diagnosticsWin.MenuController">
   <children>
      <MenuBar fx:id="menuBar" maxWidth="Infinity" onKeyPressed="#handleKeyInput">
         <menus>
            <Menu text="File">
               <items>
                  <MenuItem text="New" />
                  <SeparatorMenuItem />
                  <MenuItem text="Exit" />
               </items>
            </Menu>
            <Menu text="Help">
               <items>
                  <MenuItem onAction="#handleAboutAction" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</VBox>
